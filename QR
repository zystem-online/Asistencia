<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <title></title>
</head>
<body>
<script>
  const AS_URL_BASE = "https://script.google.com/macros/s/AKfycbxmpr9PrhxFm-BDq7RZ1X6ytsxBC-m72oMHNnaJlEJ33Sm1MQsPZtcgItdbsIhMYSgYdA/exec";
    
  var url = document.location.href;
  var k = url.substr(url.lastIndexOf("k=") + 2);
    
  if (k) {
    fetch(AS_URL_BASE + '?k=' + k)
      .then(r => r.text())
      .then((r) => {
        document.body.innerHTML = r; // reemplaza el contenido del cuerpo del documento con la respuesta del script
      })
      .catch(err => console.log(err))
  }
</script>
</body>
</html>
